{% extends "layout.html" %}

{% block content %}

<table class='table table-hover'>
  <tr>
    <th>Name</th>
    <th>Token</th>
  </tr>
  {% for person in people %}
    <tr>
      <td>{{ person.name }}</td>
      <td><a href="/sphere/{{ person.token }}">{{ person.token }}</a></td>
    </tr>
  {% endfor %}
</table>

{% endblock %}
